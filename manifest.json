{
    "name": "Daily Journal Chrome Extension",
    "description": "A Chrome Extension to Keep Track of Daily Activites.",
    "version": "2.3",
    "manifest_version": 3,
    "permissions": ["alarms", "storage", "activeTab"],
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "128": "128IconNoBG.png"
    },
    "background": {
      "service_worker": "background.js"
    }
}
  